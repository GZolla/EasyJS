{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include":["base","addOns","addOns/displays","addOns/testing","README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "doc-templates/easydoc",
        "destination": "./docs" 
    }
}