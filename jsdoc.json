{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include":["base","addOns"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "templates": "templates/default",
        "destination" : "./docs"
    }
}